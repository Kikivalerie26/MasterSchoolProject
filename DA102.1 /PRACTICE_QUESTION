/*How many clicks are generated by Instagram in the age bucket 
18-24?This question is required*/
SELECT 
    SUM(clicks) AS total_clicks,
    CASE 
        WHEN age_bucket_user IN ('18-24') THEN '18-24'
        ELSE 'Other'
    END AS age_group
FROM meta_revenue
WHERE PARENT_COMPANY = 'Instagram'
GROUP BY age_group;

SELECT 
CASE WHEN age_bucket_user IN ('18-24', '25-34') THEN '18-34'
		ELSE '35+' 
 END AS age_group
 FROM meta_revenue;

SELECT
CASE WHEN age_bucket_user BETWEEN 18 AND 34 THEN '18-34' 
ELSE '35+' END AS age_group
 FROM meta_revenue
 
 
/*What is the total revenue generated by the Display ad types in 2018
(rounded to 1 decimal place)? */           
SELECT ad_types,
     SUM(revenue) AS total_revenue
FROM META_REVENUE
WHERE YEARS = 2018
--AND ad_types = 'Display'
group by ad_types
;

SELECT
SUM(revenue)
FROM
meta_revenue
WHERE
years = 2018
AND
ad_types LIKE '%Display';

Select * from meta_revenue limit 10
